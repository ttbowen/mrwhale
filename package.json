{
  "name": "mrwhale",
  "version": "1.1.0",
  "description": "This is a fun chatbot for discord.",
  "scripts": {
    "start": "npm run build && node ./dist/bot.js",
    "start:dev": "npm run build:dev && node ./dist/bot.js",
    "build": "gulp",
    "build:dev": "gulp watch",
    "test": "mocha --timeout 10000 --require ts-node/register ./test/**/*.spec.ts",
    "lint": "tslint src/**/*.ts",
    "lint:fix": "tslint --fix src/**/*.ts",
    "precommit": "lint-staged && npm run lint",
    "prettier:write": "prettier --write **/*.{ts,js}"
  },
  "lint-staged": {
    "**/*.{ts,js}": [
      "npm run prettier:write",
      "git add"
    ]
  },
  "keywords": [
    "whale",
    "discord",
    "bot"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bowenwaregames/mrwhale.git"
  },
  "author": "Thomas Bowen",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/moment-duration-format": "^2.2.0",
    "@types/node": "^10.0.3",
    "@types/request-promise": "^4.1.41",
    "@types/sequelize": "^4.27.10",
    "@types/sinon": "^4.3.0",
    "@types/sinon-chai": "^2.7.29",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^4.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "mocha": "^5.0.4",
    "mock-fs": "^4.4.2",
    "nodemon": "^1.17.1",
    "prettier": "^1.11.1",
    "sinon": "^5.0.3",
    "sinon-chai": "^3.0.0",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "tslint-no-unused-expression-chai": "^0.1.3",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "d20": "^1.4.1",
    "mock-require": "^3.0.2",
    "moment": "^2.22.1",
    "moment-duration-format": "^2.2.2",
    "node-opus": "^0.3.0",
    "reflect-metadata": "^0.1.10",
    "request-promise": "^4.2.2",
    "sequelize": "^4.37.5",
    "sql.js": "^0.5.0",
    "sqlite3": "^4.0.0",
    "typeorm": "0.2.6",
    "wikijs": "^4.6.1",
    "wolfram": "^0.3.4",
    "yamdbf": "^3.0.1",
    "ytdl": "^0.10.1"
  }
}
